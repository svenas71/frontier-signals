{
  "name": "frontier-signals",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "eleventy --serve & npx tailwindcss -i ./src/styles/tailwind.css -o ./src/assets/site.css --watch",
    "build:css": "tailwindcss -i ./src/styles/tailwind.css -o ./src/assets/site.css --minify",
    "build": "ELEVENTY_ENV=prod eleventy && npm run build:css"
  },
  "devDependencies": {
    "@11ty/eleventy": "^3.0.0",
    "@11ty/eleventy-plugin-rss": "^2.0.0",
    "@tailwindcss/typography": "^0.5.10",
    "autoprefixer": "^10.4.19",
    "luxon": "^3.5.0",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.6"
  }
}